{
	  "name": "Page Redder",
	  "description": "Make the current page red",
	  "version": "2.0",
	  "permissions": [
	    "activeTab"
	  ],
	  "content_scripts": [{
	    "js": ["jquery-1.11.3.js","test.js"],
	    "matches":  [ "http://*/*", "https://*/*"]
	  }],
	  "browser_action": {
	    "default_title": "Make this page red"
	  },
    "background":
    {
        "scripts": ["myscript.js"]
    },
    "background_page":"background.html",
	  "manifest_version": 2
	}
